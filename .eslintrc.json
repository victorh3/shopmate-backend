{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "space-before-function-paren": [
      "off"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "consistent-return": [
      "off"
    ],
    "func-names": [
      "off"
    ]
  }
}